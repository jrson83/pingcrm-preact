{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "clean": "rimraf public/js/* public/css/* public/mix-manifest.json",
        "eslint": "eslint --fix src --ext .js",
        "eslint:check": "eslint src --ext .js",
        "prettier": "prettier --write src",
        "prettier:check": "prettier --check src",
        "fix-code-style": "npm run prettier && npm run eslint",
        "client": "mix --mix-config=webpack.mix.js",
        "ssr": "mix --mix-config=webpack.ssr.mix.js",
        "ssr-server": "node public/js/ssr.js",
        "init": "mix --mix-config=webpack.mix.js && mix --mix-config=webpack.ssr.mix.js && npm-run-all --parallel dev:*",
        "development": "npm-run-all --parallel dev:*",
        "dev:nodemon": "nodemon --watch public/js --ext css,scss,sass,js,webmanifest --watch public public/js/ssr.js",
        "dev:mix-client": "mix --mix-config=webpack.mix.js watch",
        "dev:mix-server": "mix --mix-config=webpack.ssr.mix.js watch",
        "forever:start": "NODE_ENV=production forever start public/js/ssr.js",
        "forever:stop": "NODE_ENV=development forever stop public/js/ssr.js",
        "forever:restart": "NODE_ENV=production forever restart public/js/ssr.js",
        "disabled:watch": "mix watch",
        "disabled:watch-poll": "mix watch -- --watch-options-poll=1000",
        "disabled:hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production && mix --production --mix-config=webpack.ssr.mix.js",
        "heroku-postbuild": "npm run prod"
    },
    "dependencies": {
        "@babel/plugin-transform-react-jsx": "^7.16.7",
        "@inertiajs/inertia": "^0.11.0",
        "@inertiajs/progress": "^0.2.7",
        "@inertiajs/server": "^0.1.0",
        "@jrson83/inertia-preact": "^0.1.0",
        "@yandeu/prettier-config": "^0.0.3",
        "autoprefixer": "^10.4.0",
        "axios": "^0.25.0",
        "babel-preset-preact": "^2.0.0",
        "eslint": "^8.7.0",
        "eslint-config-preact": "^1.3.0",
        "eslint-plugin-jest": "^25.7.0",
        "express": "^4.17.2",
        "jest": "^27.4.7",
        "laravel-mix": "^6.0.6",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.6",
        "postcss-import": "^14.0.2",
        "postcss-nesting": "^10.1.2",
        "preact": "^10.6.4",
        "preact-render-to-string": "^5.1.19",
        "prettier": "^2.5.1",
        "prettier-plugin-tailwind": "^2.2.12",
        "rimraf": "^3.0.2",
        "tailwindcss": "^2.0.3",
        "uuid": "^8.3.2",
        "webpack-node-externals": "^3.0.0"
    }
}
